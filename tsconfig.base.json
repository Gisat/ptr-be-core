{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "resolveJsonModule": true,
    "moduleResolution": "bundler",
    "isolatedModules": true,
    "skipLibCheck": true,
    "declaration": true,
    "declarationDir": "dist",
    "outDir": "dist",
    "noImplicitAny": false,
  },
  "include": ["**/*"],
}