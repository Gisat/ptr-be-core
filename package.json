{
  "name": "@gisatcz/ptr-be-core",
  "version": "0.0.1",
  "description": "Shared core library for PTR BE services",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build:clean": "rm -rf dist .rollup.cache tsconfig.tsbuildinfo",
    "build:barrels": "barrelsby -c barrelsby.json",
    "yalc:publish": "yalc publish",
    "yalc:publish:push": "yalc publish --push",
    "dev": "rollup --config --watch",
    "build": "rollup --config",
    "test": "vitest --config vitest.config.ts"
  },
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "luxon": "^3.6.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/luxon": "^3.6.2",
    "@types/node": "^22.15.12",
    "barrelsby": "^2.8.1",
    "rollup": "^4.40.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "tsx": "^4.19.4",
    "typescript": "^5.9.3",
    "vite": "^6.2.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1"
  }
}
