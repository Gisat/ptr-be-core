{
  "name": "@gisatcz/ptr-be-core",
  "version": "0.0.1-dev.4",
  "description": "Shared core library for PTR BE services",
  "type": "module",
  "files": [
    "dist/"
  ],
  "exports": {
    "./browser": {
      "types": "./dist/index.browser.d.ts",
      "import": "./dist/index.browser.js"
    },
    "./node": {
      "types": "./dist/index.node.d.ts",
      "import": "./dist/index.node.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "yalc:publish": "yalc publish",
    "yalc:publish:push": "yalc publish --push",
    "build:clean": "rm -rf dist .rollup.cache tsconfig.tsbuildinfo",
    "dev": "npm run build:clean && rollup --config --watch",
    "build": "npm run build:clean && rollup --config",
    "build:tsc": "tsc --build tsconfig.prod.json",
    "test": "vitest --config vitest.config.ts",
    "pack:check": "npm pack --dry-run"
  },
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "luxon": "^3.6.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/luxon": "^3.6.2",
    "@types/node": "^22.15.12",
    "barrelsby": "^2.8.1",
    "rollup": "^4.40.2",
    "rollup-plugin-dts": "^6.2.3",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "tsx": "^4.19.4",
    "typescript": "^5.9.3",
    "vite": "^6.2.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1"
  }
}
