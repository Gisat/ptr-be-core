{
  "name": "@gisatcz/ptr-be-core",
  "version": "0.0.5",
  "description": "Shared core library for PTR BE services",
  "repository": {
    "type": "git",
    "url": "https://github.com/Gisat/ptr-be-core.git"
  },
  "type": "module",
  "files": [
    "dist/"
  ],
  "exports": {
    "./browser": {
      "types": "./dist/index.browser.d.ts",
      "import": "./dist/index.browser.js",
      "default": "./dist/index.browser.js"
    },
    "./node": {
      "types": "./dist/index.node.d.ts",
      "require": "./dist/index.node.cjs",
      "default": "./dist/index.node.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "yalc:publish": "yalc publish",
    "yalc:publish:push": "yalc publish --push",
    "dev": "npm run build:clean && rollup --config --watch",
    "pack:check": "npm pack --dry-run",
    "lint": "eslint",
    "test": "vitest --config vitest.config.ts",
    "build:clean": "rm -rf dist .rollup.cache tsconfig.tsbuildinfo",
    "build": "npm run build:clean && rollup --config",
    "before:push": "npm run lint && npm run build && npm run test"
  },
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "lodash": "^4.17.21",
    "luxon": "^3.6.1",
    "pino": "^9.6.0"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/lodash": "^4.17.13",
    "@types/luxon": "^3.6.2",
    "@types/node": "^22.15.12",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.27.0",
    "eslint-plugin-node": "^11.1.0",
    "rollup": "^4.40.2",
    "rollup-plugin-dts": "^6.2.3",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "tsx": "^4.19.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.2.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1"
  }
}
